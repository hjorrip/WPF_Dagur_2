<Window x:Class="WPF2_0_Panels.Canvas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF2_0_Panels"
        mc:Ignorable="d"
        Title="Canvas" Height="450" Width="800">
    <Canvas>
        <Rectangle Canvas.Left="100" Canvas.Top="100" Height="50" Width="50" Fill="Red"/>
        <Rectangle Canvas.Left="120" Canvas.Top="120" Height="50" Width="50" Fill="Blue"/>
    </Canvas>
</Window>
